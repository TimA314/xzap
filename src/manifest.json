{
  "manifest_version": 3,
  "name": "XZap",
  "version": "1.0",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "assets/icon.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["file-select.html", "etch.js"],
      "matches": ["<all_urls>"]
    }
  ]
}